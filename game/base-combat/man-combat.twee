		<<elseif $rng lte 20>>
			<span class="pink"><<He_nun>> <<his_yi>> 손가락으로 당신의 자지를 감싼다.</span>
			<<neutral 15>>
			<<set $speechpenis to 1>>
			<<combat-set-hand-target $args[0] "penis">>
			<<bodypart_admire_chance pubic>>
		<</if>>
	<</widget>>

	<!-- pass "left" or "right" for which NPC hand this is called for -->
	<<widget "combat-hand-on-vagina">>
		<<set $rng to random(1, 100)>>
		<<if $rng gte 101>>
			<span class="purple"><<He_nun>> <<his_yi>> 손가락을 당신의 <<pussyPost>>에서 빼낸다.</span>
			<<combat-set-hand-target $args[0] "vaginaentrance">>
			<<neutral 15>>
		<<elseif $rng lte 100>>
			<<He_nun>> 당신의 <<pussyPost>> 안쪽을 <<strokes and>>,
			<<neutral 20>><<set $speechvagina to 1>>
			<<if $arousal gte ($arousalmax / 5) * 4>>
				무심결에 당신의 몸을 전율시킨다.
			<<elseif $arousal gte $arousalmax / 5>>
				당신의 음란한 액체를 흘러나오게 한다.
			<<else>>
				당신의 몸을 가장 추잡한 방식으로 침범한다.
			<</if>>
			<<bodypart_admire_chance pubic>>
		<</if>>
	<</widget>>

	<<widget "combat-hand-on-penis">>
		<<set $rng to random(1, 100)>>
		<<if $rng gte 101>>
			<span class="purple"><<He_nun>> 당신의 <<penis_rul>> <<his_yi>> 손아귀에서 놓는다.</span>
			<<combat-set-hand-target $args[0] "penisentrance">>
			<<neutral 15>>
		<<elseif $rng lte 100>>
			<<He_nun>> 당신의 <<penis_rul>> 아래에서 위까지 문질러서,
			<<neutral 20>><<set $speechpenis to 1>>
			<<if $arousal gte ($arousalmax / 5) * 4>>
				무심결에 당신의 몸을 전율시킨다.
			<<elseif $arousal gte $arousalmax / 5>>
				당신의 음란한 액체를 흘러나오게 한다.
			<<else>>
				당신의 몸의 자연스러운 반응을 가장 추잡한 방식으로 이용한다.
			<</if>>
			<<bodypart_admire_chance pubic>>
		<</if>>
	<</widget>>

	<<widget "combat-hand-on-anusentrance">>
		<<set $rng to random(1, 100)>>
		<<if $rng gte 101>>
			<span class="blue"><<He_nun>> <<his_yi>> 손을 당신의 엉덩이에서 뗀다.</span>
			<<combat-reset-hand $args[0]>>
			<<set $vaginause to 0>>
		<<elseif $rng gte 21 or $noanal is 1>>
			<<He_nun>> 당신의 엉덩이를 <<strokes>>.
			<<neutral 10>>
			<<if random(1, 2) is 2>>
				<<bodypart_admire_chance left_bottom>>
			<<else>>
				<<bodypart_admire_chance right_bottom>>
			<</if>>
		<<elseif $rng lte 20 and $noanal is 0>>
			<span class="pink"><<He_nun>> 당신의 항문 안에 손가락 하나를 밀어넣는다.</span>
			<<combat-set-hand-target $args[0] "anus">>
			<<neutral 15>>
			<<set $speechanus to 1>>
			<<if random(1, 2) is 2>>
				<<bodypart_admire_chance left_bottom>>
			<<else>>
				<<bodypart_admire_chance right_bottom>>
			<</if>>
		<</if>>
	<</widget>>

	<<widget "combat-hand-on-anus">>
		<<set $rng to random(1, 100)>>
		<<if $rng gte 101>>
			<span class="purple"><<He_nun>> <<his_yi>> 손가락을 당신의 항문에서 빼낸다.</span>
			<<combat-set-hand-target $args[0] "anusentrance">>
			<<neutral 15>>
		<<elseif $rng lte 100>>
			<<He_nun>> 리드미컬하게 <<his_yi>> 손가락을 당신의 항문에 넣었다 뺐다 하면서,
			<<neutral 20>><<set $speechanus to 1>>
			<<if $player.gender is "f">>
				당신의 항문을 희롱한다.
			<<else>>
				당신의 전립선을 희롱한다.
			<</if>>
			<<if random(1, 2) is 2>>
				<<bodypart_admire_chance left_bottom>>
			<<else>>
				<<bodypart_admire_chance right_bottom>>
			<</if>>
		<</if>>
	<</widget>>

	<<widget "combat-hand-on-mouth">>
		<<set $rng to random(1, 100)>>
		<<if $enemyanger lte 20 and $enemytrust gte 20>>
			<span class="teal">당신의 협조적인 태도에 자신감을 갖고, <<he_nun>> <<his_yi>> 손을 당신의 입에서 뗀다.</span>
			<<combat-reset-hand $args[0]>>
			<<set $mouthuse to 0>>
		<<else>>
			<<He_nun>> 계속 당신의 입을 꽉 잡아 막고 있다.
			<<violence 1>><<bruise face>>
			<<bodypart_admire_chance forehead>>
		<</if>>
	<</widget>>

	<<widget "combat-hand-on-overlowerclothes">>
		<<set $rng to random(1, 100)>>
		<<if $worn.over_lower.name is "naked">>
			<span class="purple"><<He_nun>> 그 천조각을 옆으로 치운다.</span>
			<<combat-reset-hand $args[0]>>
		<<elseif $overlowerstruggle is 1>>
			<<set $overlowerstruggle to 0>>
			당신은 어떻게든 <<him_i>> 당신의 <<worn_over_lower_name_ul>> 벗기는 것을 막아낸다.
			<<set $speechstripstruggle to 1>>
		<<elseif $rng gte 101>>
			<<He_nun>> <<his_yi>> 손에서 당신의 <<worn_over_lower_name_ul>> 놓는다.
			<<combat-reset-hand $args[0]>>
		<<elseif $rng lte 100>>
			<<if $worn.over_lower.skirt is 1>>
				<<if $worn.over_lower.skirt_down is 1>>
					<<He_nun>> 당신의 스커트를 들어올려, 당신의
					<<if $worn.lower.state is "waist">>
						<<worn_lower_name_ul>> 드러낸다.
						<<neutral 2>>
					<<elseif $worn.under_lower.state is "waist">>
						<<worn_under_lower_name_ul>> 드러낸다.
						<<neutral 2>>
					<<elseif $worn.genitals.name isnot "naked">>
						<<worn_genitals_name_ul>> 드러낸다.
						<<neutral 2>>
					<<else>>
						<span class="lewd"><<genitals_rul>> 드러낸다.</span>
						<<neutral 5>><<set $speechgenitals to 1>>
					<</if>>
					<<set $worn.over_lower.skirt_down to 0>><<set $worn.over_lower.vagina_exposed to 1>><<set $worn.over_lower.anus_exposed to 1>><<set $worn.over_lower.exposed to 2>>
				<<elseif $worn.over_lower.skirt_down is 0>>
					<<if _overOutfit>>
						<<if $worn.over_upper.state is "waist">>
							<<if $position is "wall">>
								<<He_nun>> 당신의 <<worn_over_upper_name_ul>> 세게 잡아당겨, <span class="blue">손을 놓아주기</span> 전에 옷이 약간 찢어진다.
								<<combat-reset-hand $args[0]>><<neutral 1>><<set $worn.over_lower.integrity -= 10>>
							<<else>>
								<<He_nun>> 당신의 <<worn_over_upper_name_ul>> 당신의 몸통 위로 들어올린다.
								<<set $worn.over_lower.state to "midriff">><<set $worn.over_upper.state to "midriff">><<neutral 1>>
								<<if $worn.over_upper.state_top is "waist">>
									<<set $worn.over_upper.state_top to "midriff">>
								<</if>>
							<</if>>
						<<elseif $worn.over_upper.state is "midriff">>
							<<He_nun>> 당신의 <<worn_over_upper_name_ul>> 들어올려,
							<<if $worn.upper.name isnot "naked">>
								당신의 <<worn_upper_name_ul>> 드러낸다.
							<<else>>
								<span class="lewd">당신의 <<breasts_rul>> 드러낸다.</span><<set $speechbreasts to 1>>
