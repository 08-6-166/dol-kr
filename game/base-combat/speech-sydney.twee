		<</switch>>
	<<elseif $rng gte 75>>
		<<switch _sydneyStatus>>
			<<case "pure" "pureLust">>
				<<set _sydneyText to `<<He>> takes a deep breath. "I... I can do this."`>>
			<<case "corrupt" "corruptLust">>
				<<set _sydneyText to `<<He_ga>> 말한다. "Think you can keep up?"`>>
			<<default>>
				<<set _sydneyText to `<<He_ga>> 말한다. "I think I can keep pace with you, now."`>>
		<</switch>>
	<<elseif $rng gte 70>>
		<<switch _sydneyStatus>>
			<<case "pure" "pureLust">>
				<<set _sydneyText to `<<He>> takes a deep breath. "We... need <<printTo>> stay quiet..."`>>
