<<widget "farm_text_manyPost">><<silently>>
<<if $args[0] is "horse">>
	<<if $farm_work[$args[0]].monster_roll is true and (($hallucinations gte 1 or $monsterhallucinations is "f") or $bestialitydisable is "t")>>
		<<set _trResult to "켄타우로스들">>
	<<else>>
		<<set _trResult to "말들">>
	<</if>>
<<elseif $args[0] is "pig">>
	<<if $farm_work[$args[0]].monster_roll is true and (($hallucinations gte 1 or $monsterhallucinations is "f") or $bestialitydisable is "t")>>
